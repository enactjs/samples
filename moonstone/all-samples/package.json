  "dependencies": {
    "@enact/core": "^4.0.4",
    "@enact/i18n": "^4.0.4",
    "@enact/moonstone": "^4.0.1",
    "@enact/spotlight": "^4.0.4",
    "@enact/ui": "^4.0.4",
    "@enact/webos": "^4.0.4",
    "ilib": "^14.9.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "web-animations-js": "^2.3.2"
