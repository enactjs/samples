    "extends": "enact/strict"
  },
  "dependencies": {
    "@enact/core": "^1.0.0-beta.1",
    "@enact/i18n": "^1.0.0-beta.1",
    "@enact/moonstone": "^1.0.0-beta.1",
    "@enact/spotlight": "^1.0.0-beta.1",
    "@enact/ui": "^1.0.0-beta.1",
    "@enact/webos": "^1.0.0-beta.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
